import React from 'react';
import './index.css';
import { Container, Row, Col } from 'react-bootstrap';
import { TwitterTweetEmbed } from 'react-twitter-embed';

function tweetCol(tweet) {
  return (
    <Col lg="12">
      <TwitterTweetEmbed key={tweet.tweet_id} tweetId={`${tweet.tweet_id}`} />
    </Col>
  );
}

